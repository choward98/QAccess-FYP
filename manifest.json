{
    "name": "QAccess",
    "version": "1.0",
    "manifest_version": 2,
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "js": ["content/content.js"]
    }],
    "background": {
        "scripts": ["background/background.js"]
    },
    "browser_action": {
        "default_popup": "html/index.html",
        "default_title": "QAcess",
        "default_icon": {
        "19": "images/logo.png",
        "38": "images/logo.png"
        }
    },
    "permissions": [
        "tabs",
        "activeTab",
        "storage"
    ]
}
